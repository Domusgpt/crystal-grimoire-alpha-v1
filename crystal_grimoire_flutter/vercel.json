{
  "version": 2,
  "public": false,
  "github": {
    "silent": true
  },
  "framework": null,
  "buildCommand": "echo 'Flutter web app pre-built'",
  "outputDirectory": "build/web",
  "installCommand": "echo 'No install needed'",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}